{
  "expo": {
    "name": "SevApp",
    "slug": "sevapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sevapp.app"
    },
    "android": {
      "package": "com.sevapp.app",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "sounds": [
            "./assets/noti.mp3"
          ]
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "exp",
    "extra": {
      "supabaseUrl": "https://isoydimyquabqfrezuuc.supabase.co",
      "supabaseAnonKey": "sb_publishable_aNWSLMDoVdG2eo3Py9kPsg_KbrmIAyD",
      "eas": {
